{
	"name": "cuotadera",
	"description": "Cuotadera -- a community for woodworkers",
	"keywords": ["cuotadera", "woodworking", "community"],
	"homepage": "http://www.cuotadera.net/",
	"type": "project",
	"license": "BSD-3-Clause",
	"support": {
		"issues": "https://github.com/arsdehnel/cuotaderanet/issues?state=open",
		"source": "https://github.com/arsdehnel/cuotaderanet"
	},
	"minimum-stability": "alpha",
	"require": {
		"php": ">=5.4.0",
		"yiisoft/yii2": "*",
		"yiisoft/yii2-swiftmailer": "*",
		"yiisoft/yii2-bootstrap": "*",
		"yiisoft/yii2-debug": "*",
		"yiisoft/yii2-gii": "*"
	},
	"scripts": {
		"post-create-project-cmd": [
			"yii\\composer\\Installer::setPermission"
		]
	},
	"extra": {
		"writable": [
			"backend/runtime",
			"backend/web/assets",

			"console/runtime",
			"console/migrations",

			"frontend/runtime",
			"frontend/web/assets"
		]
	}
}
